  "homepage": "https://github.com/50ra4/clasp-send-mail-sample#readme",
  "devDependencies": {
    "@google/clasp": "2.3.0",
    "@types/google-apps-script": "1.0.26",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "commander": "7.1.0",
    "dotenv": "8.2.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "gas-webpack-plugin": "1.2.2",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "ts-jest": "26.5.1",
    "ts-loader": "8.0.17",
    "ts-node": "9.1.1",
    "typescript": "4.2.2",
    "webpack": "4.46.0",
    "webpack-cli": "4.5.0"
  },
  "dependencies": {
    "date-fns": "2.17.0"
